{
  "name": "@gaubee/meilisearch-sdk",
  "version": "1.0.1",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "type": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "type": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "keywords": [
    "meilisearch",
    "server",
    "sdk",
    "cli"
  ],
  "author": "Gaubee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gaubee/meilisearch-sdk.git"
  },
  "scripts": {
    "build": "tsc --build ./tsconfig.json"
  },
  "bin": {
    "meilisearch": "./dist/esm/cli.js"
  },
  "optionalDependencies": {
    "@gaubee/meilisearch-linux-aarch64": "workspace:*",
    "@gaubee/meilisearch-linux-amd64": "workspace:*",
    "@gaubee/meilisearch-macos-amd64": "workspace:*",
    "@gaubee/meilisearch-macos-apple-silicon": "workspace:*",
    "@gaubee/meilisearch-windows-amd64": "workspace:*"
  }
}